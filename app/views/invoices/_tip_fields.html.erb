<div class="nested-fields">
  <div class="row">
    <div class="col s3">
      <label>Value</label>
      <%= f.number_field :value, class: 'tip_field' %>
    </div>
    <div class="col s8">
      <label>Staff</label>
      <%= f.select :staff_id, @staff, {}, {class: 'browser-default select2'} %>
    </div>
    <div class="col s1">
      <%= link_to_remove_association "x", f %>
    </div>
  </div>
</div>



<script>
    setTimeout(function () {
        $('.tip_field').keyup(calculateSums);
        $('.select2').select2();

    }, 50)

</script>