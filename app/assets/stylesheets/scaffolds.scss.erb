@import "variables";
@font-face {
  font-family: 'Cairo';
  src: font-url("Cairo-Regular.ttf") format('ttf');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: 'Arvo';
  src:
          font-url('Arvo-Regular.ttf') format('ttf'),
          font-url('Arvo-Bold.ttf') format('tff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Mulish';
  src:
          font-url('Mulish-Regular.ttf') format('ttf'),
          font-url('Mulish-Bold.ttf') format('tff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Leef';
  src: font-url('PermanentMarker-Regular.ttf') format('ttf');
  font-weight: normal;
  font-style: normal;
}
body, p, ol, ul, td, a , h1, h2, h3, h4, h5, h6, header{
//  font-family: Leef !important;
}
body, p, ol, ul, td {
  font-size: 16px;
  line-height: 20px; }

pre {
  background-color: #eee;
  padding: 10px;
  font-size: 11px; }

a {
  color: $light-blue; }
